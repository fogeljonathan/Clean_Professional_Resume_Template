%--------SKILLS------------
\section{Skills}
 \resumeSubHeadingListStart
   \resumeItem{}
     {\textbf{Proficient In}: LIST THINGS YOU KNOW WELL}
   \resumeItem{}
     {\textbf{Experience In}: LIST THINGS YOU KNOW}
   \resumeItem{}
     {\textbf{Interests}: LIST SOME INTERESTS}
 \resumeSubHeadingListEnd

