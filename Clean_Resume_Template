\begin{document}

%----------HEADING-----------------
\begin{tabular*}{\textwidth}{l@{\extracolsep{\fill}}r}
  \textbf{\href{YOUR LINKEDIN LINK HERE(Include https://)}{\Large First Last}} & Email : \href{mailto:YOUR EMAIL HERE}{YOUREMAIL@MAIL.com}\\
  
  
  
  \href{YOUR LINKEDIN LINK HERE(Include https://)}{YOUR LINKEDIN LINK} &     \href{YOUR GITHUB LINK HERE(Include https://)}{YOUR GITHUB LINK}\\
  
   & Mobile : YOURPHONENUMBER
\end{tabular*}
%-----------Summary-----------------

%-----------EDUCATION-----------------
\section{Education}
  \resumeSubHeadingListStart
    \resumeSubheading
      {NAME OF COLLEGE OR UNI}{WHERE}
      {YOUR MAJOR}{Aug. 2017 -- May 2021}
      \resumeItemListStart
        \resumeItem{Physics:}
          {GPA: \textbf{YOUR GPA} /4.00. \textbf{Relevant Coursework:} LIST RELEVANT COURSEWORK HERE.}

      \resumeItemListEnd
  \resumeSubHeadingListEnd


%-----------EXPERIENCE-----------------
